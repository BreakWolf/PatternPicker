{
    "FileSelectionSetting": {
        "IncludedExtionsions":[
            "aspx",
            "cs",
            "html",
            "htm"
        ]
    },

    "PatternPickerSetting":{
        "RegEx": {
            "Match":"([\u300e\u4e00-\u9fa5\u300f\uff01-\uff20\u3105-\u3126 \\-\\!]+)",
            "NotMatch":[
                "\\W"
            ]
        },
        "LeftCommentMark": [
            "/*",
            "<!--"
        ],
        "RightCommentMark":[
            "*/",
            "-->"
        ],
        "CommentMark": [
            "//",
            "#region"
        ],
        "OutputFile": "output.csv"
    }
}